{
  "name": "@x-library/vue-router",
  "version": "0.1.0",
  "type": "module",
  "description": "Vue Router扩展功能，支持自动路由生成和布局系统",
  "main": "./src/index.ts",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./index.ts"
    },
    "./rsbuild": {
      "types": "./dist/rsbuild/index.d.ts",
      "require": "./dist/rsbuild/index.js",
      "import": "./dist/rsbuild/index.mjs"
    },
    "./vite": {
      "types": "./dist/vite/index.d.ts",
      "require": "./dist/vite/index.js",
      "import": "./dist/vite/index.mjs"
    },
    "./webpack": {
      "types": "./dist/webpack/index.d.ts",
      "require": "./dist/webpack/index.js",
      "import": "./dist/webpack/index.mjs"
    },
    "./layouts": {
      "types": "./src/layouts/index.ts",
      "import": "./src/layouts/index.ts",
      "require": "./src/layouts/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest"
  },
  "keywords": [
    "vue",
    "vue-router",
    "auto-routes",
    "layouts"
  ],
  "dependencies": {
    "@x-library/core": "workspace:*",
    "fast-glob": "^3.3.3",
    "magic-string": "^0.30.17",
    "pathe": "^2.0.3",
    "rspack-plugin-virtual-module": "^1.0.1"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.4",
    "chokidar": "^4.0.3",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "unplugin": "^2.3.5",
    "vitest": "^1.0.0"
  }
}
