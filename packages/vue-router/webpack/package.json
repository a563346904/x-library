{
  "name": "@x-library/vue-router-webpack",
  "version": "0.1.0",
  "description": "Webpack plugin for automatic Vue Router route generation",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest"
  },
  "keywords": [
    "vue",
    "vue-router",
    "webpack",
    "plugin",
    "auto-routes",
    "layouts"
  ],
  "dependencies": {
    "@x-library/core": "workspace:*",
    "@x-library/vue-router-shared": "workspace:*",
    "chokidar": "^4.0.3",
    "pathe": "^2.0.3",
    "webpack-virtual-modules": "^0.6.1"
  },
  "devDependencies": {
    "@types/node": "^24.0.4",
    "@types/webpack": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1",
    "webpack": "^5.0.0"
  },
  "peerDependencies": {
    "vue": ">=3.0.0",
    "vue-router": ">=4.0.0",
    "webpack": ">=5.0.0"
  }
}
