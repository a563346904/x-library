{
  "name": "@x-library/lint",
  "version": "1.0.0",
  "description": "前端代码规范配置包：ESLint、Prettier、Commitlint、Stylelint",
  "type": "module",
  "author": "x-library",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "eslint": "./bin/eslint.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./eslint": {
      "types": "./dist/eslint/index.d.ts",
      "import": "./dist/eslint/index.js",
      "require": "./dist/eslint/index.js"
    },
    "./prettier": {
      "types": "./dist/prettier/index.d.ts",
      "import": "./dist/prettier/index.js",
      "require": "./dist/prettier/index.js"
    },
    "./commitlint": {
      "types": "./dist/commitlint/index.d.ts",
      "import": "./dist/commitlint/index.js",
      "require": "./dist/commitlint/index.js"
    },
    "./stylelint": {
      "types": "./dist/stylelint/index.d.ts",
      "import": "./dist/stylelint/index.js",
      "require": "./dist/stylelint/index.js"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@eslint/js": "^9.23.0",
    "@stylistic/stylelint-plugin": "^3.1.2",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.0.0",
    "postcss-html": "^1.6.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.0.0",
    "stylelint": "^16.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-order": "^7.0.0",
    "stylelint-scss": "^6.12.0",
    "typescript-eslint": "^8.33.1",
    "vue-eslint-parser": "^10.1.3"
  },
  "devDependencies": {
    "@commitlint/types": "^19.8.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.9.0",
    "@vitest/coverage-v8": "^3.2.2",
    "vitest": "^3.2.2"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "stylelint": "^16.0.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-scss": "^6.12.0",
    "@stylistic/stylelint-plugin": "^3.1.2",
    "postcss-html": "^1.6.0",
    "postcss-scss": "^4.0.9",
    "stylelint-order": "^7.0.0"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": true
    },
    "prettier": {
      "optional": true
    },
    "stylelint": {
      "optional": true
    },
    "stylelint-config-standard": {
      "optional": true
    },
    "stylelint-config-rational-order": {
      "optional": true
    },
    "stylelint-scss": {
      "optional": true
    },
    "@stylistic/stylelint-plugin": {
      "optional": true
    },
    "postcss-html": {
      "optional": true
    },
    "postcss-scss": {
      "optional": true
    },
    "stylelint-order": {
      "optional": true
    }
  }
}
